<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	
	<servlet>
		<servlet-name>Order2</servlet-name>
		<servlet-class>servlet.Order2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Order2</servlet-name>
		<url-pattern>/order2</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>add_items</servlet-name>
    	<servlet-class>servlet.AddItemsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>add_items</servlet-name>
    	<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>edit_items</servlet-name>
    	<servlet-class>servlet.EditItemsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>edit_items</servlet-name>
    	<url-pattern>/edit</url-pattern>
	</servlet-mapping>
		
		
	<servlet>
    	<servlet-name>del_items</servlet-name>
    	<servlet-class>servlet.DelItemsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>del_items</servlet-name>
    	<url-pattern>/del</url-pattern>
	</servlet-mapping>
	
</web-app>
